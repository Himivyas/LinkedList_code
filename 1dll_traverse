#include <bits/stdc++.h>
using namespace std;

struct node{
   public:
    int data;
    node* next;

  public:
    node(int data1, node* next1){
      data = data1;
      next = next1;
  }
  
  public:
    node(int data1){
        data = data1;
        next = nullptr;
    }
};

node* arrtoll(vector<int> &arr){
      node* head = new node(arr[0]);
    node* mover = head;
    for (int i = 1; i<arr.size(); i++){
        node* temp = new node(arr[i]);
        mover->next = temp;
        mover = temp;
    }
    
    return head;
}


int main(){
    vector<int> arr = {12,4,6,8};
    node* head = arrtoll(arr);
    node* trav = head;
    while (trav != nullptr){
        cout << trav-> data<< " ";
        trav = trav-> next;
    }
}



